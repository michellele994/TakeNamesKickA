<div class = "col-md-4">
	<h2>Names Taken</h2>
	<div class="row">
		<ul>
			{{#each asses}}
				<div class="col-md-3">
					{{#unless ass_kicked}}
						<img src="http://via.placeholder.com/75x75">
						{{> asses/ass-block ass_kicked=true}}
					{{/unless}}
				</div>
			{{/each}}
		</ul>
	</div>
</div>
<div class = "col-md-4">
	<h2>Take a Name</h2>
	<form class = "create-form">
		<div class = "form-group">
			<label for = "ass_name">Enter Name: </label>
			<input type = "text" id="ass_name" name="name">
		</div>
		<div class ="row">
			<div class="col-md-3">
				<img src="http://via.placeholder.com/75x75">
			</div>
			<div class="col-md-3">
				<img src="http://via.placeholder.com/75x75">
			</div>
			<div class="col-md-3">
				<img src="http://via.placeholder.com/75x75">
			</div>
			<div class="col-md-3">
				<img src="http://via.placeholder.com/75x75">
			</div>
		</div>
		<button type="submit">Take Name</button>
	</form>
</div>
<div class = "col-md-4">
	<h2>Asses Kicked</h2>

	<ul>
		{{#each asses}}
			<div class = "col-md-3">
				{{#if ass_kicked}}
					<img src="http://via.placeholder.com/75x75">
					{{> asses/ass-block ass_kicked=false}}
				{{/if}}
			</div>
		{{/each}}
	</ul>
</div>